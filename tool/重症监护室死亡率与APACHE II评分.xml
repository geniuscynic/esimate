<?xml version="1.0" encoding="utf-8"?>
<Template code="重症监护室死亡率与APACHE II评分" title="重症监护室死亡率与APACHE II评分">
<Group>
<RadioBox code="00" title="患者有器官功能不全病史?" desc="患者有器官功能不全病史?">
<Option code="00immuno" text="是的，且为急诊手术后转入是的，且为非手术后转入是的，且为择期手术后转入否" score="6"/>
</RadioBox>
<CheckBox code="01" title="患者有急性肾功能衰竭?" desc="患者有急性肾功能衰竭?">
<Option code="00arf" text="是" score="1"/>
</CheckBox>
<Text code="02" title="年龄" desc="年龄">
<Option code="00age" text="岁" score="0"/>
</Text>
<Text code="03" title="体温（肛温，摄氏度）" desc="体温（肛温，摄氏度）">
<Option code="00temp" text="°C 或 °F (其中一个!)" score="0"/>
</Text>
<Text code="04" title="平均动脉压(MAP)" desc="平均动脉压(MAP)">
<Option code="00map" text="" score="0"/>
</Text>
<Text code="05" title="动脉血 pH" desc="动脉血 pH">
<Option code="00ph" text="" score="0"/>
</Text>
<Text code="06" title="心率" desc="心率">
<Option code="00hr" text="bpm" score="0"/>
</Text>
<Text code="07" title="呼吸频率" desc="呼吸频率">
<Option code="00rr" text="bpm" score="0"/>
</Text>
<Text code="08" title="钠 （血清）" desc="钠 （血清）">
<Option code="00na" text="mmol/L" score="0"/>
</Text>
<Text code="09" title="钾 （血清）" desc="钾 （血清）">
<Option code="00pot" text="mmol/L" score="0"/>
</Text>
<Text code="10" title="肌酐 （血清）" desc="肌酐 （血清）">
<Option code="00cr" text="μmol/L" score="0"/>
</Text>
<Text code="11" title="血球压积" desc="血球压积">
<Option code="00hct" text="" score="0"/>
</Text>
<Text code="12" title="白细胞计数" desc="白细胞计数">
<Option code="00wbc" text="x103cells / mm3" score="0"/>
</Text>
<Text code="13" title="格拉斯哥昏迷评分" desc="格拉斯哥昏迷评分">
<Option code="00gcs" text="分" score="0"/>
</Text>
<Text code="14" title="肺泡-动脉氧分压差（如FiO2 ≥ 0.5请输入）" desc="肺泡-动脉氧分压差（如FiO2 ≥ 0.5请输入）">
<Option code="00aagrad" text="mm Hg" score="0"/>
</Text>
<Text code="15" title="PaO2 (如 FiO2 < 0.5请输入)" desc="PaO2 (如 FiO2 < 0.5请输入)">
<Option code="00pao2" text="mm Hg" score="0"/>
</Text>
</Group>
</Template>